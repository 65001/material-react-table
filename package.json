{
  "version": "0.4.5",
  "license": "MIT",
  "name": "material-react-table",
  "description": "A fully featured Material-UI implementation of react-table, inspired by material-table and the mui DataGrid, written from the ground up in TypeScript.",
  "author": "Kevin Vandy",
  "keywords": [
    "react-table",
    "material-ui",
    "material-table"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinVandy/material-react-table"
  },
  "homepage": "https://www.material-react-table.com/",
  "bugs": {
    "url": "https://github.com/KevinVandy/material-react-table/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build && size-limit",
    "build-storybook": "build-storybook",
    "format": "prettier -w .",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsdx test --passWithNoTests",
    "upgrade": "ncu -u && npm i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint && format"
    }
  },
  "module": "dist/material-react-table.esm.js",
  "size-limit": [
    {
      "path": "dist/material-react-table.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/material-react-table.esm.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@etchteam/storybook-addon-status": "^4.2.0",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-storysource": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/faker": "^6.6.8",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-table": "^7.7.0",
    "size-limit": "^7.0.8",
    "storybook-addon-paddings": "^4.2.1",
    "storybook-dark-mode": "^1.0.9",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@mui/icons-material": ">=5",
    "@mui/material": ">=5",
    "react": ">=16.8",
    "react-table": ">=7"
  }
}
