{
  "version": "0.13.3",
  "license": "MIT",
  "name": "material-react-table",
  "description": "A fully featured Material UI implementation of TanStack React Table, inspired by material-table and the MUI X DataGrid, written from the ground up in TypeScript.",
  "author": "Kevin Vandy",
  "keywords": [
    "react-table",
    "material-ui",
    "material-table",
    "tanstack table"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinVandy/material-react-table"
  },
  "homepage": "https://www.material-react-table.com/",
  "bugs": {
    "url": "https://github.com/KevinVandy/material-react-table/issues"
  },
  "main": "dist/index.js",
  "module": "dist/material-react-table.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build && size-limit && rm -rf material-react-table-docs/node_modules/material-react-table/dist && cp -r dist material-react-table-docs/node_modules/material-react-table/ && cp -r src material-react-table-docs/node_modules/material-react-table/ && cp -r package.json material-react-table-docs/node_modules/material-react-table/package.json",
    "build-storybook": "build-storybook",
    "format": "prettier -w .",
    "lint": "eslint .",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint && format"
    }
  },
  "size-limit": [
    {
      "path": "dist/material-react-table.cjs.production.min.js",
      "limit": "55 KB"
    },
    {
      "path": "dist/material-react-table.esm.js",
      "limit": "55 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@faker-js/faker": "^6.3.1",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.4",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^18.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "size-limit": "^7.0.8",
    "storybook-dark-mode": "^1.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=5",
    "@mui/material": ">=5",
    "react": ">=16.8"
  },
  "dependencies": {
    "@tanstack/match-sorter-utils": "^8.0.0-alpha.83",
    "@tanstack/react-table": "^8.0.0-beta.9",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-virtual": "^2.10.4"
  }
}
